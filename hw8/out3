get_verb: oak
(['had', 'a', 'group', 'of', 'oak', 'trees'], [Tree('VP', [Tree('VBD', ['had']), Tree('NP', [Tree('NP', [Tree('DT', ['a']), Tree('NN', ['group'])]), Tree('PP', [Tree('IN', ['of']), Tree('NP', [Tree('NN', ['oak']), Tree('NNS', ['trees'])])])])])])
